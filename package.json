    "eslint": ">=5.0.0"
  },
  "repository": "JamieMason/eslint-formatter-git-log",
  "scripts": {
    "build": "tsc --project .",
    "format": "prettier --write '**/*.md' './src/**/*.ts' './test/**/*.ts'",
    "lint": "tslint --fix --project ./tsconfig.json",
